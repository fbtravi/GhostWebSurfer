# Docker Environment Variables for GhostWebSurfer
# Copy this file to .env when using docker-compose
# Usage: docker-compose --env-file .env up

# Target URL to test
TARGET_URL=https://google.com/

# Total number of users to simulate
TOTAL_USERS=5

# Maximum number of concurrent users
CONCURRENCY=5

# Additional wait time after page load (milliseconds)
WAIT_MS=2000

# Page load timeout in milliseconds
PAGE_LOAD_TIMEOUT_MS=60000

# Output mode: 'file' or 'dashboard'
OUTPUT_MODE=file
